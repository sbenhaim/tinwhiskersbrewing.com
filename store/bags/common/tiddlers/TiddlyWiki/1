	color: [[ColorPalette::Background]];
	background-color: [[ColorPalette::Foreground]];
}

#sidebarTabs .tiddlyLinkNonExisting {
	font-style: italic;
}

/*
 * story
 */

.tiddler {
	margin-bottom: 10px;
	border: 1px solid [[ColorPalette::TertiaryPale]];
	padding-bottom: 10px;
}

