$ curl -X PUT -H 'Content-Type: application/json' \
	-d '{ "desc": "lorem ipsum", "policy": { "read": [], "write": ["ANY"], "create": ["ANY"], "delete": ["NONE"], "manage": ["R:ADMIN"], "accept": ["R:ADMIN"], "owner": "administrator" } }' \
	http://localhost:8080/bags/Alpha
}}}</pre>
